<%- include("./partials/header") -%>
<h1>Home</h1>
<p><%= homeStartingContent %></p>

<% posts.forEach(function(post){ %>
    <div class="post">
        <h2><%= post.title %></h2>
        <p><%= post.content.substring(0, 100) + "..."%></p>
        <a href="/posts/<%= post.title %>">Read more</a>
    </div>
<% }); %>
<%- include("./partials/footer") -%>